{
  "functions": {
    "api/sync.js": {
      "maxDuration": 30
    },
    "api/archive.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/sync",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/archive",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/scripts/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/styles/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
